{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier", "airbnb-base"],
    "env": {
        "es2021": true,
        "node": true,
        "commonjs": true
    },
    "rules": {
        //"no-console": "error"
        "no-useless-constructor": "off",
        "indent": ["error", 3]
    }
}